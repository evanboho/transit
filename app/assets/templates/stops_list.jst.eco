<div class="stops-list scrolling-list col-md-3">
  <img class='up-arrow visible-sm-block visible-xs-block' src='/assets/up-arrow-icon-small.png'>
  <ul>
    <% for stop in @models: %>
      <li>
        <a href='#' data-stop-id='<%= stop.get('stop_code') %>'>
          <%= stop.get('name') %>
        </a>
      </li>
    <% end %>
  </ul>
</div>

<div id="departure-list-outlet"></div>
